<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lama.lamapp.EnterWord2">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:id="@+id/ActivityEnterword2_Textview_title"
            android:layout_width="268dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:text="Entrer les mots"
            android:textAlignment="center"
            android:textSize="28sp"
            android:textStyle="normal|bold"
            tools:layout_editor_absoluteX="46dp"
            tools:layout_editor_absoluteY="8dp"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="20dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ListView
            android:id="@+id/ActivityEnterword2_listView"
            android:layout_width="360dp"
            android:layout_height="567dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="18dp"
            android:layout_below="@+id/ActivityEnterword2_Textview_title"
            android:layout_alignParentStart="true" />

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
